def fibonacci(var lim: int): void
begin
  var result: int[] = [0, 1];
  var i: int = 1;
  
  while(result[i-1] + result[i] < lim)
    i = i + 1;
    result[i] = result[i-2] + result[i-1];
  endwhile

  var l: int = 0;
  var step: int = 1;

  for(l, i, step)
    print("%d ,", result[l]);
  endfor

end

def init(args: string[]): void
begin
  var lim: int;

  input(lim);
  fibonacci(lim);

end